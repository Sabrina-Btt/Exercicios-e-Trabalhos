#include <stdio.h>
#include <stdlib.h>


int vet[6]={5,5,1,3,4,2};

void bubble(int vet[], int n){
    int i,j,aux,cont=0;
    for(i=0;i<n-1;i++){
            for(j=1;j<n-i;j++){
                if(vet[j]<vet[j-1]){
                      aux  = vet[j];
                      vet[j] = vet[j - 1];
                      vet[j - 1] = aux;
                      cont++;
                }

            }
    }
   printf("%d",cont);
   if(cont%2 == 0)
    printf("\nCarlos\n");
   else
    printf("\nMarcelo\n");
}

int main () {
    int i;
    bubble(vet,6);
    for(i=0;i<6;i++){
            printf("%d",vet[i]);
    }
}




