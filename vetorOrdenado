#include <stdio.h>
#include <stdlib.h>
#define tam 10

int a[tam];

int buscaBinaria (int vet[], int valor, int Tam)
{
     int ini = 0;
     int fim = Tam-1;
     int meio;
     while (ini <= fim)
     {
          meio = (ini + fim)/2;
          if (valor == vet[meio])
               return meio;
          if (valor < vet[meio])
               fim = meio-1;
          else
               ini = meio+1;
     }
     return -1;
}


void printvet(int vetor[]) {
  int i;
  for(i=0; i<tam; i++) {
    printf("%d ", vetor[i]);
  }
  printf("\n");
}

void merge(int vet[], int a[], int ini, int meio, int fim){
    int idA=ini, idB=meio+1, fimA=meio, fimB=fim;
    int pos=ini,i;

    while(idA<=fimA && idB<=fimB){
        if(vet[idA]<vet[idB]){
            a[pos]=vet[idA];
            idA++;
            pos++;
        }
        else {
            a[pos]=vet[idB];
            idB++;
            pos++;
        }
    }
    if(idA>fimA){
        while(idB<=fimB){
            a[pos]=vet[idB];
            idB++;
            pos++;
        }
    }
    else{
        while(idA<=fimA){
            a[pos]=vet[idA];
            idA++;
            pos++;
        }
    }
    for(i=ini;i<=fim;i++){
        vet[i]=a[i];
    }
}

void mergeSort(int vet[],int ini,int fim){
     int meio;

     if(ini<fim){
        meio= (ini+fim)/2;
        mergeSort(vet,ini,meio);
        mergeSort(vet,meio+1,fim);
        merge(vet,a,ini,meio,fim);
    }
}




void remover(int vet[],int valor){
    int ident=-1;
    int i;
    ident=buscaBinaria(vet,valor,tam);
    if(ident!=-1){
        for(i=ident;i<vet[0];i++){
            vet[i]=vet[i+1];
        }
        vet[0]--;
    }
}

int busca(int vet[],int valor){
    int i=1;
    while(i<vet[0]){
        if(vet[i]==valor)
            return i;
        i++;
    }
    return -1;
}

void insere(int vet[],int valor){
    int ident=-1;
    ident=busca(vet,valor);
    if(ident==-1){
        vet[0]++;
        vet[vet[0]]=valor;
    }
}


void inserir(int vet[],int valor){
    int i;
    int ind = buscaBinaria(vet,valor,tam);
        if(ind != -1){
            for(i=ind;i<vet[0];i++){
               vet[i+1]=vet[i];
            }

        vet[0]++;
        }
}



void main(){
    int num,i;
    int vet[tam+5];
    FILE *f;
    f=fopen("valores.txt","r");
    vetor[0]=0;

    for(i=1;i<=(tam/2);i++){
        fscanf(f,"%d\n",&num);
        inserir(vetor,num);
    }
    int opcao,cont=(tam/2)+1;

    while(cont<=tam){
        fscanf(f,"%d ",&opcao);
        switch(opcao){
    case 0:
        fscanf(f,"%d\n",&num);
        inserir(vetor,num);
        break;
    case 1:
        fscanf(f,"%d\n",&num);
        remover(vetor,num);
        break;
        }
        cont++;
    }
}
